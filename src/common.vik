fn u32_parse(ptr: &u8, len: uint): u32 {
    let answer: u32 = 0
    for i in 0..len {
        answer = (answer * 10) + (*ptr.offset(i) - '0' as u32)
    }
    return answer
}

fn uint_parse(ptr: &u8, len: uint): uint {
    let answer = 0
    for i in 0..len {
        answer = (answer * 10) + (*ptr.offset(i) - '0' as uint)
    }
    return answer
}

fn ensure_trailing_char(file: [u8], char: u8): [u8] {
    if file[file.len() - 1] != char {
        file.push(char)
    }
    return file
}